{
  "name": "sf-salesforcegroupeseb",
  "version": "1.1.0",
  "description": "This repository contains the metadata definition of the Groupe SEB Salesforce org.",
  "main": "index.js",
  "scripts": {
    "RetrieveSourceWithDX": "sfdx force:source:retrieve -x ./scripts/sfdxSourceRetrieve/package.xml",
    "StreamLogInTerminal": "sfdx force:apex:log:tail --color ",
    "StreamDebugLogInTerminal": "sfdx force:apex:log:tail --color | grep \".*USER_DEBUG.*\"",
    "EmptySalesforceDebugLog": "sfdx force:data:soql:query -q \"SELECT Id FROM ApexLog\" -r \"csv\" > apexLog.csv && sfdx force:data:bulk:delete -s ApexLog -f .\\apexLog.csv",
    "ManualDeploymentOfPackage.xml": "sfdx force:source:deploy -x \"./scripts/ManualDeploymentOfPackage/package.xml\" --testlevel RunSpecifiedTests -r \"skip\"",
    "picklist-class-generation": "npx ts-node ./scripts/nodejs/PicklistValuesApexClassGenerator.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/groupeseb/SF-SalesforceGroupeSEB.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/groupeseb/SF-SalesforceGroupeSEB/issues"
  },
  "homepage": "https://github.com/groupeseb/SF-SalesforceGroupeSEB#readme",
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "fs": "0.0.1-security",
    "glob": "^7.1.7",
    "grep-cli": "^0.2.1",
    "husky": "^4.3.0",
    "prettier": "^2.1.2",
    "prettier-plugin-apex": "^1.7.0",
    "pretty-quick": "^3.0.2",
    "remove-accents": "^0.4.2",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.4"
  },
  "dependencies": {
    "sfdx-cli": "^7.82.1-0"
  }
}
